{
  "name": "politico-react",
  "version": "1.0.0",
  "description": "A platform where voters can vote for their desired candidates.",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "eslint:check": "node_modules/eslint/bin/eslint.js \"src/**/*.jsx\"",
    "prettier:check": "prettier --check \"./src/**/*.jsx\"",
    "prettier:fix": "prettier --write \"./src/**/*.jsx\"",
    "check:lint-prettier": "npm run eslint:check && npm run prettier:check",
    "pretest": "npm run check:lint-prettier",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "coveralls": "jest --coverage && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sojida/politico-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sojida/politico-react/issues"
  },
  "homepage": "https://github.com/sojida/politico-react#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.8.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@trendmicro/react-sidenav": "^0.4.5",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "jest-css-modules-transform": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-notify-toast": "^0.5.0",
    "react-router-dom": "^5.0.0",
    "react-sidenav": "^3.1.9",
    "styled-components": "^4.2.0",
    "uniqid": "^5.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
